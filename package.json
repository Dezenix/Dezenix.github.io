{
  "name": "app-react",
  "description": "Stable version of a basic SPA example React and TypeScript.",
  "license": "MIT",
  "version": "3.1.0",
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "yarn lint:scripts && yarn lint:styles && yarn lint:editor",
